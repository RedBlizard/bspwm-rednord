menu-0-1-exec = caffeine -d
menu-0-2 = "  "
menu-0-2-exec = caffeine kill
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/uptime]
type = custom/script
exec = uptime | awk -F, '{sub(".*up ",x,$1);print $1}'
interval = 100
label = Uptime %output%
tail = false

format-foreground = ${colors.text}
format-background = ${colors.background}
format-prefix = "    "
format-prefix-foreground = ${colors.text}
format-padding = 1
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/variety]
type = custom/script
exec = echo " Variety "
interval = 1
tail = false
format-foreground = #88C0D0
format-background = #d92e3440
format-prefix = " "
format-prefix-foreground = #88C0D0
format-prefix-background = #d92e3440
click-right = variety --preferences
click-left = variety --selector
format-padding = 1
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/nitrogen]
type = custom/script
exec = echo " Nitrogen "
interval = 1
tail = false
format-foreground = ${colors.cyan}
format-background = ${colors.background}
format-prefix = " "
format-prefix-foreground = ${colors.cyan}
format-prefix-background = ${colors.background}
click-left = /home/denise/.config/bspwm/scripts/nitrogen-wallpaper-slide.sh
click-right = nitrogen
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/filesystem]
type = internal/fs

; Mountpoints to display
mount-0 = /

; Seconds to sleep between updates
; Default: 30
interval = 10

; Display fixed precision values
; Default: false
fixed-values = true

; Spacing between entries
; Default: 2
spacing = 4

; Available tags:
;   <label-mounted> (default)
;   <bar-free>
;   <bar-used>
;   <ramp-capacity>
format-mounted = <label-mounted>

; Available tags:
;   <label-unmounted> (default)
format-unmounted = <label-unmounted>

; Available tokens:
;   %mountpoint%
;   %type%
;   %fsname%
;   %percentage_free%
;   %percentage_used%
;   %total%
;   %free%
;   %used%
; Default: %mountpoint% %percentage_free%%
label-mounted = "   %{A1:konsole -e ranger &:} %free%%{A}"
label-mounted-foreground = #88C0D0
label-mounted-background = ${colors.background}


; Available tokens:
;   %mountpoint%
; Default: %mountpoint% is not mounted
label-unmounted = %mountpoint%: not mounted
label-unmounted-foreground = ${colors.dim}
format-padding = 7

