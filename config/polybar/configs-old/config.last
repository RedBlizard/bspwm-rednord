;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;                                                                               ;;
;;                                    MODULES                                    ;;
;;                                                                               ;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

[module/launcher]
type=custom/text
content = 
content-font = 4
content-foreground = ${colors.white}
content-background = ${colors.background}
content-padding = 1
click-left=rofi -show drun
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;                                         
[module/title]
type = internal/xwindow

; Available tags:
;   <label> (default)
format = <label>
format-padding = 1
format-foreground =  ${colors.theme}
format-background = ${colors.background}
; Available tokens:
;   %title%
; Default: %title%
label = "ﱮ  %title%" 
label-maxlen = 50

; Used instead of label when there is no window title
label-empty = 
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/date]
type = internal/date
interval = 1

date = 
date-alt = "  﨟  %A, %b %d -"

time ="   %H:%M %p  "
time-alt = "%I:%M:%S %p  "

;format-prefix = ""
format-prefix-padding = 2
format-prefix-font = 2
format-prefix-foreground = #ffffff
format-background = ${colors.background}
format-foreground = ${colors.text}
label = %date% %time%
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/popup-calendar]
type = custom/script
exec = ~/.config/polybar/scripts/popup-calendar.sh
tail = false 
interval = 5
format-prefix = "    "
format-padding = 0
format-background = ${colors.background}
format-foreground = ${colors.text}
click-left = ~/.config/polybar/scripts/popup-calendar.sh --popup &
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/backlight]
type = internal/backlight
card = amdgpu_bl0
format = <ramp> <label>
format-background = ${colors.background}
format-foreground = ${colors.active}
format-padding = 2
label = %percentage%%
ramp-0 = " "
ramp-1 = " "
ramp-2 = " "
ramp-3 = " "
ramp-4 = " "
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/bspwm]
type = internal/bspwm

fuzzy-match = true

label-dimmed-urgent = ${colors.theme}
label-dimmed-foreground = ${colors.dim}
label-dimmed-underline = 
label-dimmed-focused-background = ${colors.theme}


label-focused = %name%
label-focused-foreground = ${colors.white}
label-focused-background = ${colors.theme}
;label-focused-underline= ${colors.primary}
;label-focused-foreground = ${colors.foreground-alt}
label-focused-padding = 2

label-occupied = %name%
label-occupied-background = ${colors.background}
label-occupied-foreground = ${colors.orange}
;label-occupied-underline= ${colors.cyan}
label-occupied-padding = 2

label-urgent = %name%
label-urgent-background = ${colors.red}
label-urgent-foreground = 
label-urgent-padding = 2

label-empty = %name% 
;label-empty-background = ${colors.background}
label-empty-foreground = ${colors.text}
label-empty-padding = 2

; Create scroll handlers used to cycle workspaces
; Default: true
enable-scroll = true

; Set the scroll cycle direction 
; Default: true
reverse-scroll = true


; Separator in between workspaces
label-separator = |
label-separator-foreground = ${colors.text}
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/bspwm-tmp]
type = internal/bspwm
format = <label-state>

label-active = 
label-active-foreground = ${colors.theme}
label-active-padding = 1
label-occupied = 
label-occupied-foreground = ${colors.theme}
label-occupied-padding = 1
label-empty = 
label-empty-foreground = ${colors.text}
label-empty-padding = 1
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/modicons]
type = internal/bspwm
 
format = <label-mode>
format-background = ${color.background}
format-foreground = ${colors.text}
format-padding = 1
 
label-monocle = ""
label-tiled = ""
 
label-fullscreen = " "
label-floating = " 类"
label-pseudotiled = " "
 
label-locked = " "
label-sticky = " "
label-private = " "
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/mod]
type = internal/bspwm

format = <label-mode>
format-background = ${color.background}
format-foreground = ${colors.theme}
format-padding = 1
format-font = 5

label-monocle = "[M]"
label-tiled = "[]="

label-floating = ", ><>"
label-pseudotiled = ", Pseudo"
label-fullscreen = ", Full"

label-locked = " | Locked"
label-sticky = " | Sticky"
label-private = " | Private"
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/updates]
type = custom/script
exec = ~/.config/polybar/scripts/checkupdates.sh
interval = 900
label = "%output% "
format-foreground = ${colors.green}
format-background = ${colors.background}
format-prefix = " "
format-prefix-foreground = ${colors.green}
click-left = kitty -e $SHELL -c 'sudo pacman -Syu'
click-right = bspc rule -a \* -o state=floating ; sleep 0.015 ; kitty -e $SHELL -c 'paru -Qu | less'
format-underline =
format-padding = 1 
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/network]
type = internal/network
interface = enp5s0
;interface = enp14s0
interval = 3.0


accumulate-stats = true
unknown-as-up = true

; Available tokens:
;   %ifname%    [wireless+wired]
;   %local_ip%  [wireless+wired]
;   %essid%     [wireless]
;   %signal%    [wireless]
;   %upspeed%   [wireless+wired]
;   %downspeed% [wireless+wired]
;   %linkspeed% [wired]
; Default: %ifname% %local_ip%
label-connected = "Ethernet %linkspeed%  "
label-disconnected = " Disconnected"

format-connected-foreground = ${colors.red}
format-connected-background = ${colors.background}
format-connected-underline = 
format-connected-prefix = "   "
format-connected-prefix-foreground = ${colors.red}
format-connected-prefix-background = ${colors.background}

format-disconnected = <label-disconnected>
format-disconnected-prefix = " "
format-disconnected-prefix-font = 2
format-disconnected-foreground = ${color.magenta}
format-disconnected-prefix-foreground = ${color.magenta}

label-packetloss = Packetloss " "
label-packetloss-foreground = ##B48EAD
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/nm-launcher]
type = custom/text
content = "   Nm  "
tail = false
content-background = ${colors.background}
content-foreground = ${colors.text}
click-left = nm-connection-editor
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/netspeed]
type = internal/network

; Set correct interface name for the module to work
; Try eth0 for ethernet or wlan0 for wireless
;
; You can also run ifconfig in a terminal
; to know the interace names on your system.
interface = enp5s0
interval = 1.0
label-connected = "%{T1}龍%{T-}%{T2} %downspeed% | %upspeed%%{T-}  "
label-disconnected = "%{T2}0/0%{T-}"
label-connected-background = ${colors.background}
label-connected-foreground = ${colors.text}
label-disconnected-foreground = ${colors.red}
format-padding = 2
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[module/cpu]
type = internal/cpu
interval = 1
format = <label> <bar-load>
label = CPU %percentage%%
label-foreground = ${colors.text}


format-prefix = " 龍 "
format-prefix-foreground = ${colors.theme}

bar-load-width = 10
bar-load-foreground-0 = ${colors.green}
bar-load-foreground-1 = ${colors.yellow}
bar-load-foreground-2 = ${colors.orange}
bar-load-foreground-3 = ${colors.red}
bar-load-indicator = ▐
bar-load-indicator-font = 0
bar-load-indicator-foreground = ${colors.load}
bar-load-fill = ▐
bar-load-fill-font = 0
bar-load-empty = ▐
bar-load-empty-font = 0
bar-load-empty-foreground = ${colors.load}
format-padding = 1
