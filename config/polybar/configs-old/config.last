[bar/left]
monitor =DP-0
monitor-fallback =
monitor-strict = false
enable-ipc=true
height = 32
width = 100%
bottom = false

font-0 = "SFMono Medium Nerd Font Complete:size=12;3"
font-1 = "DejaVu Sans:style=Regular:pixelsize=11;3"
font-2 = siji:pixelsize=11;3
font-3 = unifont:fontformat=truetype:size=8:antialias=false;0
font-4 =JetBrainsMono Nerd Font:style=Bold:pixelsize=13;3
font-5 ="Iosevka Term:pixelsize=11;3"
font-6 = "Iosevka Nerd Font:style=Bold:size=11;3"

offset-x = 0
offset-y = 0
radius = 0
border-size = 0
border-color = ${colors.foreground}
background = ${colors.background}
foreground = ${colors.foreground}
line-size = 2
padding=0

modules-left = bspwm modicons title
modules-center = 
modules-right = cpu memory system-nvidia-smi gputemp filesystem date spacing

scroll-up = bspwn-wsnext 
scroll-down = bspwn-wsprev
override-redirect = false
wm-restack = bspwm

;tray-position = right
;tray-padding = 4
;tray-maxsize = 22
;tray-background = ${colors.background}
;tray-foreground = ${colors.theme}

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[bar/middle]
monitor =eDP
monitor-fallback =
monitor-strict = false
enable-ipc=true
height = 32
width = 100%
bottom = false

font-0 = "SFMono Medium Nerd Font Complete:size=12;3"
font-1 = "DejaVu Sans:style=Regular:pixelsize=11;3"
font-2 = siji:pixelsize=11;3
font-3 = unifont:fontformat=truetype:size=8:antialias=false;0
font-4 =JetBrainsMono Nerd Font:style=Bold:pixelsize=13;3
font-5 ="Iosevka Term:pixelsize=11;3"
font-6 = "Iosevka Nerd Font:style=Bold:size=11;3"

offset-x = 0
offset-y = 0
radius = 0
border-size = 0
border-color = ${colors.foreground}
background = ${colors.background}
foreground = ${colors.foreground}
line-size = 2
padding=0

modules-left = bspwm modicons title
